{
  "name": "@worktile/ngx-sortablejs",
  "version": "17.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:app": "ng build --configuration production --base-href /ngx-sortablejs/",
    "build:lib": "ng build ngx-sortablejs",
    "test:app": "ng test ngx-sortablejs-app",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "gh-pages": "git reset && cpr dist/ngx-sortablejs-app docs -d  && cpr docs/index.html docs/404.html && git add docs",
    "gh-pages:apply": "yarn run gh-pages && git commit -m 'docs: update github pages' && git push",
    "release": "wpm release --release-branch-format release-v{{version}}",
    "pub-only": "cd dist/ngx-sortablejs && npm publish --access=public",
    "pub": "wpm publish && npm run pub-only"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "18.2.0",
    "@angular/common": "18.2.0",
    "@angular/compiler": "18.2.0",
    "@angular/core": "18.2.0",
    "@angular/forms": "18.2.0",
    "@angular/platform-browser": "18.2.0",
    "@angular/platform-browser-dynamic": "18.2.0",
    "@angular/router": "18.2.0",
    "bootstrap": "^4.4.1",
    "ngx-bootstrap": "^9.0.0",
    "rxjs": "^7.4.0",
    "sortablejs": "^1.10.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.2.0",
    "@angular-eslint/builder": "18.3.0",
    "@angular-eslint/eslint-plugin": "18.3.0",
    "@angular-eslint/eslint-plugin-template": "18.3.0",
    "@angular-eslint/schematics": "18.3.0",
    "@angular-eslint/template-parser": "18.3.0",
    "@angular/cli": "18.2.0",
    "@angular/compiler-cli": "18.2.0",
    "@angular/language-service": "18.2.0",
    "@semantic-release/changelog": "^3.0.6",
    "@semantic-release/git": "^7.0.18",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^8.10.59",
    "@types/sortablejs": "^1.10.2",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@worktile/pkg-manager": "^0.0.11",
    "codelyzer": "^6.0.0",
    "coveralls": "^3.0.9",
    "cpr": "^3.0.1",
    "eslint": "^8.57.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^18.2.0",
    "ngx-spec": "^2.1.4",
    "protractor": "~7.0.0",
    "semantic-release": "^15.14.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.39.1",
    "tslint": "~6.1.3",
    "typescript": "~5.5.4"
  }
}
